{
  "name": "@peercolab/engine",
  "version": "0.1.12",
  "description": "",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.mjs"
    },
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "type": "module",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --sourcemap --clean --out-dir dist",
    "test": "node -e \"process.exit(0)\"",
    "postbuild": "node scripts/rename-mjs.cjs"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/New-Horizon-Tech/peercolabengine-ts"
  },
  "keywords": [],
  "author": "New Horzion Tech",
  "license": "SEE LICENSE",
  "devDependencies": {
    "@types/node": "^24.5.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  },
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "cjs": [
        "./dist/index.d.cts"
      ],
      "esm": [
        "./dist/index.d.ts"
      ]
    }
  }
}
